; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Library"
config/description="Library of Godot templates"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

CameraManager="*res://Addons/GodotLibrary/Singletons/CameraManager/Scenes/camera_manager.tscn"
SoundManager="*res://Addons/GodotLibrary/Singletons/SoundManager/Scenes/sound_manager.tscn"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[rendering]

textures/canvas_textures/default_texture_filter=0
